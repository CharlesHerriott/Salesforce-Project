<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_in_Range</fullName>
    <active>true</active>
    <errorConditionFormula>Invoice_Submitted_Date__c &gt; TODAY() ||  Invoice_Paid_Date__c &gt; TODAY()</errorConditionFormula>
    <errorMessage>Cannot enter Invoice Submitted Date or Invoice Paid Date in the future!</errorMessage>
</ValidationRule>
